###  http://www.disit.org/km4city/schema#BusStop
km4c:BusStop rdf:type owl:Class ;
             rdfs:subClassOf km4c:TransferServiceAndRenting ,
                             otn:StopPoint ,
                             geo:SpatialThing ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty geo:lat ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:float
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty geo:long ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:float
                             ] ;
             rdfs:comment "Interconnection point between road segments to determine the path of a TPL line."@en ,
                          "Punto di interconnessione tra segmenti di strada per determinare il percorso di una linea di TPL."@it ,
                          "Service associated with displaying ATAF lines with their stops."@en ,
                          "Servizio associato alla visualizzazione delle linee ATAF con le relative fermate."@it ;
             rdfs:label "Bus Stop"@en ,
                        "Fermata bus"@it .


###  http://www.disit.org/km4city/schema#BusStopForecast
km4c:BusStopForecast rdf:type owl:Class ;
                     rdfs:subClassOf [ rdf:type owl:Restriction ;
                                       owl:onProperty km4c:atBusStop ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass km4c:BusStop
                                     ] ;
                     rdfs:comment "Arrival forecast at a certain stop."@en ,
                                  "Previsione di arrivo ad una certa fermata."@it ;
                     rdfs:label "BusStop Forecast"@en ,
                                "Previsione arrivo in fermata"@it .


