###  http://www.disit.org/km4city/schema#consistOfElement
km4c:consistOfElement rdf:type owl:ObjectProperty ;
                      rdfs:domain km4c:RailwayDirection ;
                      rdfs:range km4c:RailwayElement ;
                      rdfs:comment "A railway Direction is composed by more railway element."@en ,
                                   "Una direzione ferroviaria è formata da più elementi ferroviari."@it ;
                      rdfs:label "Consist of element"@en ,
                                 "Formato da elemento"@it .


###  http://www.disit.org/km4city/schema#containsElement
km4c:containsElement rdf:type owl:ObjectProperty ;
                     owl:inverseOf km4c:isPartOfRoad ;
                     rdfs:domain km4c:Road ;
                     rdfs:range km4c:RoadElement ;
                     rdfs:comment "A road is formed by more elements road."@en ,
                                  "Una strada è formata da più elementi stradali."@it ;
                     rdfs:label "Contains element"@en ,
                                "Contiene elemento"@it .


###  http://www.disit.org/km4city/schema#containsSite
km4c:containsSite rdf:type owl:ObjectProperty ;
