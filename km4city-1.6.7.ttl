###  http://schema.org/location
schema:location rdf:type owl:ObjectProperty ;
                rdfs:domain schema:Event ;
                rdfs:range schema:Place ;
                rdfs:comment "Property of the schema.org ontology."@en ,
                             "Proprietà dell'ontologia schema.org."@it ;
                rdfs:label "Location"@en ,
                           "Locazione"@it .


###  http://www.disit.org/km4city/schema#accessToElement
km4c:accessToElement rdf:type owl:ObjectProperty ;
                     owl:inverseOf km4c:hasRule ;
                     rdfs:domain km4c:EntryRule ;
                     rdfs:range km4c:RoadElement ;
                     rdfs:comment "An entry rule refers to a specific Road element."@en ,
                                  "Una regola di accesso si riferisce ad uno specifico elemento stradale."@it ;
                     rdfs:label "Accede all'elemento"@it ,
                                "Access to element"@en .


###  http://www.disit.org/km4city/schema#approvedByPa
km4c:approvedByPa rdf:type owl:ObjectProperty ;
                  owl:inverseOf km4c:hasResolution ;
                  rdfs:domain km4c:Resolution ;
                  rdfs:range km4c:Pa ;
                  rdfs:comment "Collega una delibera all'ente pubblico che la ha emanata."@it ,
                               "To connect a resolution to the municipality that have approved it."@en ;
                  rdfs:label "Approvato dalla PA"@it ,
                             "Approved by PA"@en .


###  http://www.disit.org/km4city/schema#arrangedOnRoad
km4c:arrangedOnRoad rdf:type owl:ObjectProperty ;
                    rdfs:domain km4c:Beacon ;
                    rdfs:range km4c:Road ;
                    rdfs:comment "Collega il lampione alla strada su cui è installato."@it ,
                                 "To connect each beacon to the road in which it is installed."@en ;
                    rdfs:label "Arranged on road"@en ,
                               "Installato nella strada"@it .


###  http://www.disit.org/km4city/schema#atBusStop
km4c:atBusStop rdf:type owl:ObjectProperty ;
               owl:inverseOf km4c:hasForecast ;
               rdfs:domain km4c:BusStopForecast ;
               rdfs:range km4c:BusStop ;
               rdfs:comment "Each forecast refers to a specific bus stop."@en ,
                            "Ogni previsione si riferisce ad una specifica fermata."@it ;
               rdfs:label "Alla fermata"@it ,
                          "At bus stop"@en .


###  http://www.disit.org/km4city/schema#beginsAtJunction
km4c:beginsAtJunction rdf:type owl:ObjectProperty ;
                      rdfs:domain km4c:RouteLink ;
                      rdfs:range km4c:RouteJunction ;
                      rdfs:comment "Ciascun tratto della linea ha una giunzione di inizio e una giunzione di fine."@it ,
                                   "Each routeLink, has a beginning and an end junction."@en ;
                      rdfs:label "Begins at junction"@en ,
                                 "Inizia alla giunzione"@it .


###  http://www.disit.org/km4city/schema#belongToElement
km4c:belongToElement rdf:type owl:ObjectProperty ;
                     rdfs:domain km4c:Entry ;
                     rdfs:range km4c:RoadElement ;
                     rdfs:comment "Questa proprietà indica il segmento stradale su cui si trova l'accesso."@it ,
                                  "This property indicates the segment of road where an entry point of a street number is located."@en ;
                     rdfs:label "Appartiene al segmento stradale"@it ,
                                "Belong to element"@en .


###  http://www.disit.org/km4city/schema#belongToMunicipality
km4c:belongToMunicipality rdf:type owl:ObjectProperty ;
                          owl:inverseOf km4c:hasDistrict ;
                          rdfs:domain km4c:District ;
                          rdfs:range km4c:Municipality ;
                          rdfs:comment "A municipality can be divided in districts."@en ,
                                       "Un comune può essere suddiviso in frazioni o quartieri."@it ;
                          rdfs:label "Appartiene al comune"@it ,
                                     "Belong to municipality"@en .


###  http://www.disit.org/km4city/schema#belongToRoad
km4c:belongToRoad rdf:type owl:ObjectProperty ;
                  owl:inverseOf km4c:hasStreetNumber ;
                  rdfs:domain km4c:StreetNumber ;
                  rdfs:range km4c:Road ;
                  rdfs:comment "Each road has street numbers."@en ,
                               "Ogni strada ha i numeri civici."@it ;
                  rdfs:label "Appartiene alla strada"@it ,
                             "Belong to road"@en .


###  http://www.disit.org/km4city/schema#coincideWith
km4c:coincideWith rdf:type owl:ObjectProperty ;
                  rdfs:domain km4c:Road ;
                  rdfs:range km4c:AdministrativeRoad ;
                  rdfs:comment "Non c'è una corrispondenza esatta tra toponimi ed estese amministrative."@it ,
                               "There is no exact match between Road and AdministrativeRoad."@en ;
                  rdfs:label "Coincide con"@it ,
                             "Coincide with"@en .


###  http://www.disit.org/km4city/schema#composeDirection
km4c:composeDirection rdf:type owl:ObjectProperty ;
