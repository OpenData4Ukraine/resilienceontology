                 rdfs:range km4c:PublicTransportLine ;
                 rdfs:comment "An AVMrecord concert to a unique line."@en ,
                              "Una rilevazione AVM si riferisce ad una specifica linea del TPL."@it ;
                 rdfs:label "Concern line"@en ,
                            "Riguarda la linea"@it .


###  http://www.disit.org/km4city/schema#concerningNode
km4c:concerningNode rdf:type owl:ObjectProperty ;
                    rdfs:domain km4c:Maneuver ;
                    rdfs:range km4c:Node ;
                    rdfs:comment "A maneuver takes place in correspondence of a node."@en ,
                                 "Una manovra è eseguita in corrispondenza di un nodo."@it ;
                    rdfs:label "Concerning node"@en ,
