###  http://www.disit.org/km4city/schema#AMQPBroker
km4c:AMQPBroker rdf:type owl:Class ;
                rdfs:subClassOf km4c:IoTBroker ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty km4c:amqpQueue ;
                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:string
                                ] ;
                rdfs:comment "It represents a hardware/software component that acts as an interface between a device or a set of devices (sensors, actuators) and the external, implementing the AMQP protocol."@en ,
                             "Modella un componente hardware/software che agisce da interfaccia tra un dispositivo o un insieme di dispositivi (sensori, attuatori) e il mondo esterno implementando il protocollo AMQP."@it ;
                rdfs:label "AMQP Broker"@en ,
                           "Broker AMQP"@it ;
                rdfs:seeAlso "https://en.wikipedia.org/wiki/Advanced_Message_Queuing_Protocol"@en ,
                             "https://it.wikipedia.org/wiki/Advanced_Message_Queuing_Protocol"@it .


###  http://www.disit.org/km4city/schema#AVMRecord
km4c:AVMRecord rdf:type owl:Class ;
               rdfs:subClassOf geo:SpatialThing ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty km4c:onRoute ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass km4c:Route
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty geo:lat ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:float
                               ] ,
